    "nanoid": "^3.3.8",
    "postcss": "^8.4.49",
    "tough-cookie": "^4.1.3",
    "webpack": "5.94.0",
    "webpack-bundle-analyzer/ws": "^7.5.10",
    "webpack-dev-server/express": "^4.21.0",
    "webpack-dev-server/ws": "^8.18.0"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "lint-staged": {
    "*.{ts,tsx,mts}": "eslint --cache --fix -c ./.eslintrc.js",
    "*.css": "stylelint --fix",
    "*": "prettier --ignore-unknown --write"
  }
}
